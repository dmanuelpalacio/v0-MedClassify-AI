{
  "version": 2,
  "public": true,
  "functions": {
    "api/predict.py": {
      "runtime": "python3.9"
    },
    "api/predict-batch.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/api/predict-batch",
      "dest": "/api/predict-batch.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  }
}
